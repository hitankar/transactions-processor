{
	"name": "bench-test",
	"version": "1.0.0",
	"description": "Node apps",
	"author": "Hitankar",
	"license": "MIT",
	"scripts": {
		"build-ts": "tsc",
		"build": "yarn build-ts && yarn lint",
		"debug": "yarn build && yarn watch-debug",
		"lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
		"test": "jest --forceExit --silent --detectOpenHandles",
		"test:ci": "jest --forceExit --detectOpenHandles --coverage --runInBand --reporters='default' --ci --silent",
		"start": "yarn build && node ./dist/app.js"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"dotenv": "8.2.0",
		"node": ">=15.3.0",
		"winston": "3.3.3",
		"yarn": ">=1.22.17"
	},
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"@types/jquery": "3.5.5",
		"@types/lodash": "^4.14.170",
		"@types/node": "~15.3.0",
		"@types/winston": "2.4.4",
		"@typescript-eslint/eslint-plugin": "4.14.2",
		"@typescript-eslint/parser": "4.14.2",
		"eslint": "7.19.0",
		"jest": "^27.0.6",
		"nodemon": "^2.0.7",
		"ts-jest": "^27.0.3",
		"ts-node": "9.1.1",
		"typescript": "4.1.3"
	}
}