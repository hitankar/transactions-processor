{
	"name": "bench-test",
	"version": "1.0.0",
	"description": "Node apps",
	"author": "Hitankar",
	"license": "MIT",
	"scripts": {
		"build-ts": "tsc",
		"build": "yarn build-ts && yarn lint",
		"debug": "yarn build && yarn watch-debug",
		"lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
		"test": "set NODE_ENV=test & jest --forceExit --verbose --detectOpenHandles",
		"test:ci": "NODE_ENV=test jest --forceExit --detectOpenHandles --coverage --runInBand --reporters='default' --ci --silent",
		"start": "yarn build && node ./dist/app.js"
	},
	"dependencies": {
		"axios": "~0.24.0",
		"dotenv": "8.2.0",
		"node": ">=15.3.0",
		"winston": "3.3.3",
		"yarn": ">=1.22.17"
	},
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"@types/node": "~15.3.0",
		"@typescript-eslint/eslint-plugin": "5.7.0",
		"@typescript-eslint/parser": "5.7.0",
		"eslint": "8.5.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"jest": "^27.0.6",
		"prettier": "^2.5.1",
		"ts-jest": "^27.0.3",
		"ts-node": "10.4.0",
		"typescript": "4.5.4"
	}
}
